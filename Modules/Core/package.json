{
    "name": "@snd-app/core",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "main": "resources/js/index.ts",
    "types": "resources/js/index.ts",
    "sideEffects": false,
    "exports": {
        ".": {
            "types": "./resources/js/index.ts",
            "import": "./resources/js/index.ts"
        },
        "./components/ui": {
            "types": "./resources/js/components/ui/index.ts",
            "import": "./resources/js/components/ui/index.ts"
        },
        "./components/ui/*": {
            "types": "./resources/js/components/ui/*.tsx",
            "import": "./resources/js/components/ui/*.tsx"
        },
        "./layouts": {
            "types": "./resources/js/layouts/index.ts",
            "import": "./resources/js/layouts/index.ts"
        },
        "./layouts/*": {
            "types": "./resources/js/layouts/*.tsx",
            "import": "./resources/js/layouts/*.tsx"
        }
    },
    "dependencies": {
        "@radix-ui/react-dialog": "^1.0.5",
        "@radix-ui/react-dropdown-menu": "^2.0.6",
        "@radix-ui/react-slot": "^1.0.2",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.0",
        "lucide-react": "^0.323.0",
        "tailwind-merge": "^2.2.1",
        "tailwindcss-animate": "^1.0.7"
    }
} 